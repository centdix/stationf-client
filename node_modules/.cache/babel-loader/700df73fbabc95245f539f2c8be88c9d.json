,KAAA,CAAA,QAAA,GAAA,IAAA,CAAA,QAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,GA1BA;AA2BA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,GADA,EACA,IADA,EACA;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,GAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,GAAA,CAAA,CAAA,CAAA,CAAA,IAAA,KAAA,IAAA,EACA,OAAA,CAAA;AACA;;AACA,aAAA,CAAA,CAAA;AACA,KAPA;AAQA,IAAA,WARA,yBAQA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,WADA,GACA,MAAA,CAAA,UADA;AAEA,gBAAA,SAFA,GAEA,MAAA,CAAA,QAFA;;AAAA,sBAGA,WAAA,KAAA,IAAA,IAAA,SAAA,KAAA,IAAA,IAAA,WAAA,CAAA,QAAA,CAAA,IAAA,MAAA,EAAA,CAHA;AAAA;AAAA;AAAA;;AAIA,gBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,8BAAA,EAAA,MAAA,CAAA,IAAA;;AAJA;;AAAA;AAOA,gBAAA,SAPA,sBAOA,MAAA,CAAA,KAPA;AAQA,gBAAA,GARA,GAQA,wBARA;AAAA;AAAA,uBASA,KAAA,CAAA,GAAA,GAAA,UAAA,EAAA;AAAA,kBAAA,MAAA,EAAA;AAAA,iBAAA,CATA;;AAAA;AASA,gBAAA,QATA;AAAA;AAAA,uBAUA,QAAA,CAAA,IAAA,EAVA;;AAAA;AAUA,gBAAA,IAVA;AAWA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA,CAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,CAAA,EAAA;AACA,sBAAA,KAAA,GAAA,MAAA,CAAA,CAAA,CAAA,UAAA,CAAA,CAAA,QAAA,CAAA,CAAA,EAAA,SAAA,CAAA;AACA,sBAAA,GAAA,GAAA,MAAA,CAAA,CAAA,CAAA,QAAA,CAAA,CAAA,GAAA,CAAA,CAAA,EAAA,SAAA,CAAA;;AACA,sBAAA,WAAA,CAAA,SAAA,CAAA,KAAA,EAAA,GAAA,KAAA,SAAA,CAAA,SAAA,CAAA,KAAA,EAAA,GAAA,CAAA,EACA;AACA,wBAAA,CAAA,GAAA,MAAA,CAAA,UAAA,CAAA,SAAA,EAAA,CAAA,CAAA,SAAA,CAAA;;AACA,oBAAA,SAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;AACA;AACA,iBARA;;AASA,gBAAA,MAAA,CAAA,eAAA,GAAA,SAAA;;AArBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBA;AA9BA,GA3BA;AA2DA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,IAAA,GAAA,KAAA,IAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,UAAA,UAAA,GAAA,IAAA;AACA,UAAA,KAAA,IAAA,CAAA,MAAA,GAAA,CAAA,IAAA,IAAA,CAAA,MAAA,GAAA,CAAA,EACA,UAAA,GAAA,IAAA,MAAA,CAAA,KAAA,IAAA,EAAA,KAAA,CAAA,IAAA,CAAA,CAAA,CAAA,EAAA,OAAA,CAAA,IAAA,CAAA,CAAA,CAAA,CAAA;AACA,aAAA,UAAA;AACA,KAPA;AAQA,IAAA,QARA,sBAQA;AACA,UAAA,QAAA,GAAA,KAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,UAAA,QAAA,GAAA,IAAA;;AACA,UAAA,KAAA,UAAA,IAAA,IAAA,IAAA,QAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,QAAA,QAAA,GAAA,KAAA,UAAA,CAAA,KAAA,EAAA;AACA,QAAA,QAAA,CAAA,GAAA,CAAA,QAAA,CAAA,QAAA,CAAA,CAAA,CAAA,EAAA,EAAA,CAAA,EAAA,OAAA;AACA,QAAA,QAAA,CAAA,GAAA,CAAA,QAAA,CAAA,QAAA,CAAA,CAAA,CAAA,EAAA,EAAA,CAAA,EAAA,SAAA;AACA;;AACA,aAAA,QAAA;AACA;AAjBA;AA3DA,CAAA","sourcesContent":["<template>\n  <div id=\"app\" class=\"container\">\n    <h1 class=\"text-center\">Book a meeting room</h1>\n    <div id=\"date_search\">\n      <h3 class=\"text-center m-4\">Search by date and time</h3>\n      <div class=\"row mt-4 justify-content-center\">\n        <label class=\"col-3\">When: </label>\n        <input type=\"date\" id=\"date\" class=\"col-4\" v-model=\"date\">\n        <vue-timepicker :minute-interval=\"10\" v-model=\"time\" class=\"tp\"></vue-timepicker>\n      </div>\n      <div class=\"row mt-4 justify-content-center\">\n        <label class=\"col-3\">Duration: </label>\n        <vue-timepicker\n          :hour-range=\"[0, 1, 2, 3, 4, 5]\"\n          :minute-interval=\"30\"\n          v-model=\"duration\"\n          class=\"tp\">    \n        </vue-timepicker>\n        <button @click=\"date_search\" class=\"btn-secondary col-2 offset-md-2\">Search</button>\n      </div>\n    </div>\n    <RoomsList :start_date=\"start_date\" :end_date=\"end_date\"\n      :available_rooms=\"available_rooms\" @book=\"date_search\"></RoomsList>\n    <hr>\n    <BookingsList :bookings=\"bookings\"></BookingsList>\n  </div>\n</template>\n\n<script>\n  import VueTimepicker from 'vue2-timepicker';\n  import 'vue2-timepicker/dist/VueTimepicker.css';\n  import moment from 'moment';\n  import RoomsList from './components/RoomsList';\n  import BookingsList from './components/BookingsList';\n\n  export default {\n    name: 'App',\n    components: {\n      VueTimepicker,\n      RoomsList,\n      BookingsList\n    },\n    data() {\n      return {\n        date: \"\",\n        time: \"\",\n        duration: \"\",\n        available_rooms: [],\n        rooms: [],\n        bookings: [],\n      }\n    },\n    async mounted() {\n      let url = 'http://localhost:8000/';\n      let response = await fetch(url + 'rooms', { method: 'get' });\n      let data = await response.json();\n      this.rooms = data.rooms;\n      response = await fetch(url + 'bookings', { method: 'get' })\n      data = await response.json();\n      console.log(data);\n      this.bookings = data.bookings;\n    },\n    methods: {\n      findByName(arr, name) {\n        for (let i = 0; i < arr.length; i++) {\n          if (arr[i].name === name)\n            return i;\n        }\n        return -1;\n      },\n      async date_search() {\n        let start_value = this.start_date;\n        let end_value = this.end_date;\n        if (start_value === null || end_value === null || start_value.isBefore(new moment())) {\n          this.$toasted.error('Invalid or incomplete inputs').goAway(1500);\n          return ;\n        }\n        let available = [...this.rooms];\n        let url = 'http://localhost:8000/';\n        let response = await fetch(url + 'bookings', { method: 'get' })\n        let data = await response.json();\n        this.bookings = data.bookings;\n        this.bookings.forEach((b) => {\n          let start = moment(b.start_date).subtract(1, 'minutes');\n          let end = moment(b.end_date).add(1, 'minutes');\n          if (start_value.isBetween(start, end) || end_value.isBetween(start, end))\n          {\n            let i = this.findByName(available, b.room_name);\n            available.splice(i, 1);\n          }\n        })\n        this.available_rooms = available; \n      },\n    },\n    computed: {\n      start_date() {\n        let time = this.time.split(':');\n        let start_date = null;\n        if (this.date.length > 0 && time.length > 1)\n          start_date = new moment(this.date).hours(time[0]).minutes(time[1]);\n        return start_date;\n      },\n      end_date() {\n        let duration = this.duration.split(':');\n        let end_date = null;\n        if (this.start_date != null && duration.length > 1) {\n          end_date = this.start_date.clone();\n          end_date.add(parseInt(duration[0], 10), 'hours');\n          end_date.add(parseInt(duration[1], 10), 'minutes');\n        }\n        return end_date;\n      }\n    }\n  }\n</script>\n\n<style>\n  #app {\n    font-family: Avenir, Helvetica, Arial, sans-serif;\n    -webkit-font-smoothing: antialiased;\n    -moz-osx-font-smoothing: grayscale;\n    color: #2c3e50;\n    margin-top: 30px;\n  }\n  .tp {\n    flex: 0 0 25%;\n    max-width: 100%;\n  }\n</style>\n"],"sourceRoot":"src"}]}